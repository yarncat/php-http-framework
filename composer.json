{
    "name": "yarncat/php-http-framework",
    "type": "project",
    "description": "project to create a psr-compatible http-framework",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Framework\\": "src/Framework/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\App\\": "tests/App/",
            "Test\\Framework\\": "tests/Framework/"
        }
    },
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "*"
    },
    "scripts": {
        "phpcs": "phpcs",
        "tests": "phpunit --colors=always"
    }
}
